{
  "name": "tabata-master",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "npm-run-all -s build:sw build:svelte",
    "build:svelte": "svelte-kit build",
    "build:sw": "cross-env NODE_ENV=production node esbuild.config.js",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.24",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.38.2",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.5",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.8.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "classnames": "^2.3.1",
    "workbox-broadcast-update": "^6.2.4",
    "workbox-cacheable-response": "^6.2.4",
    "workbox-core": "^6.2.4",
    "workbox-expiration": "^6.2.4",
    "workbox-routing": "^6.2.4",
    "workbox-strategies": "^6.2.4",
    "workbox-window": "^6.2.4"
  }
}
