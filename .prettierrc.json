{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "importOrder": [
    "^$app/(.*)",
    "^$components/(.*)",
    "^$features/(.*)",
    "^$store/(.*)",
    "^$utils/(.*)",
    "^$constants/(.*)",
    "^\\..*"
  ],
  "importOrderSeparation": true,
  "experimentalBabelParserPluginsList": ["jsx", "typescript"]
}
